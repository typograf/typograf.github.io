{
    "parserOptions": {
      "ecmaVersion": 2018,
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "rules": {
        "semi": [2, "always"],
        "quotes": [2, "single"],
        "no-console": [0],
        "curly": [
          2,
          "all"
        ],
        "keyword-spacing": [
          2,
          {}
        ],
        "space-before-function-paren": [
          2,
          "never"
        ],
        "array-bracket-spacing": [
          2,
          "never",
          {
            "singleValue": true
          }
        ],
        "key-spacing": [
          2,
          {
            "beforeColon": false,
            "afterColon": true
          }
        ],
        "space-unary-ops": [
          2,
          {
            "words": false,
            "nonwords": false
          }
        ],
        "space-infix-ops": 2,
        "no-with": 2,
        "no-multiple-empty-lines": 2,
        "brace-style": [
          2,
          "1tbs",
          {
            "allowSingleLine": true
          }
        ],
        "eol-last": 2,
        "indent": [
          2,
          4,
          {
            "SwitchCase": 1
          }
        ],
        "react/jsx-no-target-blank": 0,
        "react/prop-types": 0,
        "react/no-unknown-property": 0
    },
    "env": {
        "node": true,
        "browser": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ]
}
